{
  "name": "knight-server",
  "version": "1.0.0",
  "description": "为其他服务提供rpc",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=knight-server",
    "stop": "egg-scripts stop --title=knight-server",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "debug-test": "npm run test-local -- --inspect",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "cov": "egg-bin cov",
    "lint": "tslint --fix app/**/*.ts config/**/*.ts",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "clean": "ets clean"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "egg": "^2.13.0",
    "egg-alinode": "^2.0.1",
    "egg-bin": "^4.9.0",
    "egg-cors": "^2.1.0",
    "egg-grpc": "^1.0.3",
    "egg-mongoose": "^2.2.0",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.10.0",
    "egg-sequelize": "^4.2.0",
    "egg-ts-helper": "^1.14.0",
    "egg-validate": "^1.1.1",
    "egg-view-nunjucks": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mysql2": "^1.6.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.111",
    "@types/mongoose": "^5.3.1",
    "@types/node": "^10.5.2",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-ci": "^1.10.0",
    "egg-mock": "^3.19.2",
    "tslint": "^5.11.0",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "yuedun",
  "license": "MIT"
}
